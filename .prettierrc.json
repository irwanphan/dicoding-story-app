{
    "semi": true,
    "useTabs": false,
    "tabWidth": 4,
    "printWidth": 100,
    "singleQuote": true,
    "quoteProps": "as-needed",
   
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
   
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "css"
}